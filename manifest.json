{
  "name": "PowPink",
  "version": "1.0",
  "manifest_version": 2,
  "description": "For a pink PowPing experience",
  "content_scripts": [
    {
      "matches": ["*://powping.com/*"],
      "css": ["main.css"]
    },
    {
      "matches": ["*://powping.com/discover/active"],
      "css": ["active.css"]
    }
  ]
}
